@model IEnumerable<FoodConciseViewModel>
@{
    ViewData["Title"] = "All Meals";
}

<h2>All Meals</h2>

<hr />
<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Category</th>
            <th>Price</th>
            <th>Description</th>
            <th>DishWeight</th>
        </tr>
    </thead>
    <tbody>
        @Html.DisplayForModel()
    </tbody>
</table>